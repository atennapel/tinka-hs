the <l> (A : Type l) (x : A) : A = x;
(~:) = the;

id = \<l> {A}. the <l> A;

const <l> {A B : Type l} (x : A) (y : B) : A = x;

compose <l> {A B C : Type l} (f : B -> C) (g : A -> B) (x : A) : C = f (g x);
rcompose <l> {A B C : Type l} (f : A -> B) (g : B -> C) (x : A) : C = g (f x);
(<<) = compose;
(>>) = rcompose;
