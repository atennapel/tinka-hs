if : (A : Type) -> Bool -> A -> A -> A
  = \A b t f. (elim Bool) (\_. A) t f b;

not : Bool -> Bool = \b. if Bool b False True;

and : Bool -> Bool -> Bool
  = \a b. if Bool a b False;

or : Bool -> Bool -> Bool
  = \a b. if Bool a True b;

xor : Bool -> Bool -> Bool
  = \a b. if Bool a (not b) b;

neqBool = xor;
eqBool : Bool -> Bool -> Bool = \a b. not (neqBool a b);
